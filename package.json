{
  "name": "boris_poehland_webseite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run sitemap:build && npm run sanity:build && npm run web:build",
    "start": "concurrently \"npm run sanity:start\" \"npm run web:start\"",
    "lint": "next lint",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx}' 'pages/**/*.{ts,tsx,js,jsx}'",
    "husky:prepare": "husky install",
    "sanity:build": "cd cms && sanity build ../public/cms -y && cd ..",
    "sanity:start": "cp .env.local ./cms/.env.development && cd cms && sanity start",
    "sitemap:build": "node sitemap.js",
    "web:build": "next build",
    "web:start": "next start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sendgrid/mail": "^7.6.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "concurrently": "^6.4.0",
    "intersection-observer": "^0.12.0",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "next": "^12.0.7",
    "next-sanity": "^0.4.0",
    "next-seo": "^4.28.1",
    "nextjs-sitemap-generator": "^1.3.1",
    "nprogress": "^0.2.0",
    "prettier": "^2.5.1",
    "puppeteer": "^12.0.1",
    "rate-limiter-flexible": "^2.3.6",
    "react": "17.0.2",
    "react-bootstrap": "^2.0.3",
    "react-codepen-embed": "^1.0.2",
    "react-cool-inview": "^2.0.8",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.20.5",
    "react-icons": "^4.3.1",
    "react-moment": "^1.1.1",
    "react-scrollmagic": "^2.3.0",
    "react-syntax-highlighter": "^15.4.5",
    "react-uid": "^2.3.1",
    "recoil": "^0.5.2",
    "sass": "^1.44.0",
    "sweetalert2": "^11.3.0",
    "use-memo-one": "^1.1.2"
  },
  "devDependencies": {
    "@sanity/cli": "^2.22.3",
    "@types/jquery": "^3.5.9",
    "@types/lodash": "^4.14.177",
    "@types/mkdirp": "^1.0.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.37",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/waypoints": "^4.0.4",
    "eslint": "8.4.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "typescript": "4.5.2"
  }
